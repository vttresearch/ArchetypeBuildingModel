{
    "objects": [
        [
            "building_process",
            "A2AHP",
            "An air-to-air heat pump with a reference COP of 2.5 at 7/35 C source/sink temperatures, loosely based on EN 15316-4-2:2017 Annex C example values."
        ],
        [
            "building_process",
            "A2WHP_radiators",
            "An air-to-water heat pump with a reference COP of 3 at 7/35 C source/sink temperatures, loosely based on EN 15316-4-2:2017 Annex C example values. Heat distribution system assumes a linear heating curve from (60C@-30C) to (20C@20C) for the sink, typical for regular radiators."
        ],
        [
            "building_process",
            "A2WHP_floor",
            "An air-to-water heat pump with a reference COP of 3 at 7/35 C source/sink temperatures, loosely based on EN 15316-4-2:2017 Annex C example values. Heat distribution system assumes a linear heating curve from (40C@-30C) to (20C@20C) for the sink, typical for floor heating systems."
        ],
        [
            "building_process",
            "A2WHP_DHW",
            "An air-to-water heat pump with a reference COP of 3 at 7/35 C source/sink temperatures, loosely based on EN 15316-4-2:2017 Annex C example values. Used for heating domestic hot water up to a fixed 60C, topped to 80C by an electric heater."
        ],
        [
            "building_process",
            "G2WHP_radiators",
            "A ground-to-water heat pump with a reference COP of 3.7 at 10/45 C source/sink temperatures, loosely based on EN 15316-4-2:2017 Annex C example values. Heat distribution system assumes a linear heating curve from (60C@-30C) to (20C@20C) for the sink, typical for regular radiators."
        ],
        [
            "building_process",
            "G2WHP_floor",
            "A ground-to-water heat pump with a reference COP of 3.7 at 10/45 C source/sink temperatures, loosely based on EN 15316-4-2:2017 Annex C example values. Heat distribution system assumes a linear heating curve from (40C@-30C) to (20C@20C) for the sink, typical for floor heating systems."
        ],
        [
            "building_process",
            "G2WHP_DHW",
            "A ground-to-water heat pump with a reference COP of 3.7 at 10/45 C source/sink temperatures, loosely based on EN 15316-4-2:2017 Annex C example values. Used for heating domestic hot water up to a fixed 60C, topped to 80C by an electric heater."
        ]
    ],
